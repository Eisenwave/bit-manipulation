function bool_to_int(x: Bool) -> Int {
    return 1 if x else 0;
}

static_assert(bool_to_int(false) == 0);
static_assert(bool_to_int(true) == 1);

function log10(x: Int) -> Int {
    return 0 if x < 10
      else 1 if x < 100
      else 2 if x < 1000
      else 3 if x < 10000
      else 4 if x < 100000
      else 5;
}

static_assert(log10(0) == 0);